{
  "name": "chat-dys",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "init-server": "tsnd --project server/tsconfig.json --respawn --pretty --transpile-only server/functions/api.ts",
    "start": "npx wrangler pages dev -- pnpm run dev"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@vueuse/core": "^10.6.1",
    "cors": "^2.8.5",
    "deez-components": "^0.3.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "gpt-tokenizer": "^2.1.2",
    "lodash.clonedeep": "^4.5.0",
    "markdown-it": "^13.0.2",
    "markdown-it-prism": "^2.3.0",
    "openai": "^4.16.1",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@iconify/json": "^2.2.138",
    "@rushstack/eslint-patch": "^1.5.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.4",
    "@types/lodash.clonedeep": "^4.5.8",
    "@types/markdown-it": "^13.0.5",
    "@types/node": "^18.18.8",
    "@types/prismjs": "^1.26.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.3.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.2.2",
    "unplugin-icons": "^0.17.3",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22",
    "wrangler": "^3.17.1"
  }
}
