{
  "name": "chat-dys",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "dev": "vite",
    "build:client": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --project tsconfig.app.json --noEmit",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "server": "tsx watch server/",
    "build:server": "npx tsc --project tsconfig.server.json",
    "type-check:server": "npx tsc --project tsconfig.server.json --noEmit",
    "generate:sqlite": "drizzle-kit generate:sqlite",
    "migrate": "tsx server/drizzle/migrate.ts",
    "start": "concurrently \"pnpm dev\" \"pnpm server\"",
    "build": "pnpm build:client && pnpm build:server"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@headlessui/vue": "^1.7.23",
    "@hono/node-server": "^1.13.7",
    "@hono/zod-validator": "^0.4.2",
    "@iconify-json/lucide": "^1.2.18",
    "@iconify-json/majesticons": "^1.2.1",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@tanstack/vue-query": "^5.62.7",
    "@vueuse/core": "^11.3.0",
    "arctic": "^1.9.2",
    "better-sqlite3": "^11.7.0",
    "deez-components": "0.7.3",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "hono": "4.6.10",
    "idb-keyval": "^6.2.1",
    "js-tiktoken": "^1.0.15",
    "lodash.clonedeep": "^4.5.0",
    "markdown-it": "^14.1.0",
    "openai": "^4.76.3",
    "pinia": "^2.3.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.16",
    "vue": "~3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tailwindcss/forms": "^0.5.9",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.28.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-vue": "^9.32.0",
    "highlight.js": "^11.11.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "vite": "^5.4.11",
    "vue-tsc": "^2.1.10"
  }
}
