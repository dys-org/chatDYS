{
  "name": "chat-dys",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "vite",
    "build:client": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --project tsconfig.app.json --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "server": "tsx watch server/",
    "build:server": "npx tsc --project tsconfig.server.json",
    "type-check:server": "npx tsc --project tsconfig.server.json --noEmit",
    "generate:sqlite": "drizzle-kit generate:sqlite",
    "migrate": "tsx server/drizzle/migrate.ts",
    "start": "concurrently \"pnpm dev\" \"pnpm server\"",
    "build": "pnpm build:client && pnpm build:server"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.2",
    "@ai-sdk/deepseek": "^0.1.3",
    "@ai-sdk/openai": "^1.1.2",
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@headlessui/vue": "^1.7.23",
    "@hono/node-server": "^1.13.7",
    "@hono/zod-validator": "^0.4.2",
    "@iconify-json/lucide": "^1.2.24",
    "@iconify-json/majesticons": "^1.2.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@tanstack/vue-query": "^5.64.2",
    "@vueuse/core": "^12.5.0",
    "ai": "^4.1.5",
    "arctic": "^2.3.4",
    "better-sqlite3": "^11.8.1",
    "deez-components": "0.7.4",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.6.1",
    "hono": "4.6.18",
    "idb-keyval": "^6.2.1",
    "js-tiktoken": "^1.0.16",
    "lodash.clonedeep": "^4.5.0",
    "markdown-it": "^14.1.0",
    "pinia": "^2.3.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "vue": "~3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.10.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "highlight.js": "^11.11.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vue-tsc": "^2.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "vue-demi"
    ]
  }
}
