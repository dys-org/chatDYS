{
  "name": "chat-dys",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "dev": "vite",
    "build:client": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --project tsconfig.app.json --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "server": "tsx watch server/",
    "build:server": "npx tsc --project tsconfig.server.json",
    "generate:sqlite": "drizzle-kit generate:sqlite",
    "migrate": "tsx server/drizzle/migrate.ts",
    "type-check:server": "npx tsc --project tsconfig.server.json --noEmit",
    "start": "concurrently \"pnpm dev\" \"pnpm server\"",
    "build": "pnpm build:client && pnpm build:server"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/lucide": "^1.2.7",
    "@iconify-json/majesticons": "^1.2.0",
    "@tanstack/vue-query": "^5.59.1",
    "@vueuse/core": "^11.1.0",
    "deez-components": "0.7.3",
    "idb-keyval": "^6.2.1",
    "lodash.clonedeep": "^4.5.0",
    "markdown-it": "^14.1.0",
    "pinia": "^2.2.4",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.13",
    "vue": "~3.5.11",
    "vue-router": "^4.4.5",
    "@hono/node-server": "^1.13.1",
    "@hono/zod-validator": "^0.2.2",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "arctic": "^1.9.2",
    "better-sqlite3": "^11.3.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "js-tiktoken": "^1.0.15",
    "lucia": "^3.2.1",
    "zod": "^3.23.8",
    "@anthropic-ai/sdk": "^0.28.0",
    "hono": "4.4.3",
    "openai": "^4.67.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tailwindcss/forms": "^0.5.9",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-vue": "^9.28.0",
    "highlight.js": "^11.10.0",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6",
    "@types/better-sqlite3": "^7.6.11",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "drizzle-kit": "^0.24.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "tsx": "^4.19.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.17.1",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2"
  }
}
